// appsettings содержит конфигурацию вашего приложения
// например 
// - адрес файла для записи логов
// - url внешнего ресурса для получения информации об отелях
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
